<QBE>
  <DATAMART name="Stores_for_Registry" />
    <REGISTRY pagination = "true" summaryColor="#00AAAA">
    <ENTITY name="it.eng.knowage.meta.stores_for_registry.Store">      
	<FILTERS>
		<FILTER title="Store type" field="store_type" presentation="MANUAL" /> 
		<FILTER title="Sales city" field="sales_city" presentation="COMBO" /> 	 		
	</FILTERS>   
    <COLUMNS>
		<COLUMN field="store_id" 			visible="false"	editable="false" 																/>
		<COLUMN field="store_name" 			visible="true"	editable="true"		title="store name"			editor="MANUAL"	sorter="ASC" 	/>
		<COLUMN field="store_type" 			visible="true"	editable="true"		title="store type"			editor="COMBO" 					/>
		<COLUMN field="store_city" 			visible="true"	editable="false"	title="store city" 											/>
		<COLUMN field="store_state" 		visible="true"	editable="false"	title="store state" 										/>
		<COLUMN field="florist" 			visible="true"	editable="true"		title="has florist"							 				/> 
		<COLUMN field="coffee_bar" 			visible="true"	editable="true"		title="has coffee bar"										/>
		<COLUMN field="video_store" 		visible="true"	editable="false"	title="has video store"										/>
		<COLUMN field="first_opened_date" 	visible="true"	editable="true"		title="first opened date"	editor="MANUAL" 				/>
		<COLUMN field="store_sqft" 			visible="true"	editable="true"		title="store square foot"	editor="MANUAL" 				/>
		<COLUMN field="sales_city" 			visible="true"	editable="true"		title="Foreign key - city"	editor="COMBO"	 				
								subEntity="rel_region_id_in_region" foreignKey="rel_region_id_in_region"/>
	</COLUMNS>
	  
	   <CONFIGURATIONS>
		<CONFIGURATION name="enableButtons" value="true"/>
       </CONFIGURATIONS>
	   
    </ENTITY>
	
  </REGISTRY>
</QBE>