<?xml version="1.0" encoding="windows-1250"?>
<QBE>
  <DATAMART name="Foodmart_Sales" />
  <REGISTRY pagination = "false" summaryColor="#b3e5eb">
    <ENTITY name="it.eng.spagobi.meta.Product">
      <FILTERS>
		<FILTER title="Product Subcategory" field="product_subcategory" presentation="COMBO" /> 
		<FILTER title="Brand Name" field="brand_name" presentation="MANUAL" /> 	 		
      </FILTERS>
	  <COLUMNS>		
		 <COLUMN field="product_id" title="Id." size="200" visible="true" editable ="false" /> 
		 <COLUMN field="brand_name"  title="Brand Name" size="200" visible="true" editable ="true" /> 		
		 <COLUMN field="product_family" title="Class Family" size="140" subEntity="rel_product_class_id_in_Product_class" foreignKey="rel_product_class_id_in_product_class" />	
		 <COLUMN field="product_subcategory" title="Class subcategory" size="160" dependsFrom="product_family" 
						dependsFromEntity="it.eng.spagobi.meta.Product" 
						subEntity="rel_product_class_id_in_Product_class" 
						foreignKey="rel_product_class_id_in_product_class" />									
	 </COLUMNS>
	  <CONFIGURATIONS>
		<CONFIGURATION name="enableButtons" value="true"/>
	  </CONFIGURATIONS>
    </ENTITY>
  </REGISTRY>
</QBE>
