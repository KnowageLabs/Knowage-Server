{
    "style": {
        "customToastContainer": {
            "position": "fixed",
            "right": "20px",
            "top": "20px",
            "width": "25rem",
            "z-index": "2000"
        },
        "customToastContent": {
            "background": "#B3E5FC",
            "border": "solid#B3E5FC",
            "border-radius": "4px",
            "border-width": "1px",
            "color": "#01579B",
            "padding": "1rem,box-shadow: 0 0.25rem 0.75rem rgb(0 0 0 / 10%)"
        },
        "toastIcon": {
            "font-size": "2rem"
        },
        "toastButton": {
            "background-color": "transparent",
            "color": "#01579B",
            "width": "5px",
            "margin-left": "auto"
        },
        "message": {
            "align-items": "center",
            "display": "flex",
            "justify-content": "center",
            "text-align": "center"
        }
    }
}
