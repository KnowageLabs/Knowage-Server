{
    "validations": {
        "attribute": [
            {
                "fieldName": "attributeName",
                "validators": [
                    {
                        "key": "required"
                    },
                    {
                        "key": "maximumCharacters",
                        "validator": {
                            "type": "maxLength",
                            "params": {
                                "max": 255
                            }
                        }
                    },
                    {
                        "key": "regex",
                        "validator": {
                            "type": "extendedAlphanumericRegex"
                        }
                    }
                ]
            },
            {
                "fieldName": "attributeDescription",
                "validators": [
                    {
                        "key": "required"
                    },
                    {
                        "key": "maximumCharacters",
                        "validator": {
                            "type": "maxLength",
                            "params": {
                                "max": 500
                            }
                        }
                    },
                    {
                        "key": "regex",
                        "validator": {
                            "type": "extendedAlphanumericRegex"
                        }
                    }
                ]
            },
            {
                "fieldName": "value",
                "validators": [{
                    "key": "required"
                }]
            },
            {
                "fieldName": "lov",
                "validators": []
            },
            {
                "fieldName": "lovId",
                "validators": [
                    { "key": "required", "validator": { "type": "custom-required" } }
                ]
            },
            {
                "fieldName": "multivalue",
                "validators": []
            },
            {
                "fieldName": "allowUser",
                "validators": []
            },
            {
                "fieldName": "syntax",
                "validators": []
            }
        ]
    }
}