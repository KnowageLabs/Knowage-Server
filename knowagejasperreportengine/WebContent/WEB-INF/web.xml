	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>JasperReportServlet</servlet-name>
		<url-pattern>/JasperReportServletBackEnd</url-pattern>
	</servlet-mapping>	


	<servlet-mapping>
