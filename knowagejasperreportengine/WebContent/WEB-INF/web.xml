	 <servlet>
		<servlet-name>JasperReportServlet</servlet-name>
		<servlet-class>
			it.eng.spagobi.engines.jasperreport.services.JasperReportEngineStartAction
		</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>JRImageServlet</servlet-name>
		<servlet-class>
			it.eng.spagobi.engines.jasperreport.services.JRImageServlet
		</servlet-class>
	</servlet>


	<servlet>
		<servlet-name>SpagoBIDrillServlet</servlet-name>
