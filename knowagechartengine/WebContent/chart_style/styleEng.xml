<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<STYLE name="eng" isDefault="false"> </STYLE>

<TEMPLATE>
	
	<generic>
		
		<CHART height="" width="" isCockpitEngine="false" orientation="horizontal" style="fontFamily:Arial;fontSize:12px;fontWeight:normal;backgroundColor:#FAFAFA;" styleName="eng">

			<TITLE style="align:center;fontFamily:Arial;fontSize:20px;fontWeight:bold;color:#002E57;" text="Insert your title"/>
			
			<SUBTITLE style="align:center;color:#C80752;fontFamily:Arial;fontWeight:normal;fontSize:18px;" text="Insert your subtitle"/>
			
			<EMPTYMESSAGE style="align:left;color:#FF0000;fontFamily:Arial;fontWeight:bold;fontSize:18px;" text="Insert your empty message (when data for chart does not exist)"/>
			
			<LEGEND floating='false'  layout="" position="top" show= 'false' style= "align:;fontFamily:Arial;fontSize:14px;fontWeight:normal;;borderWidth:1;color:#C80752;backgroundColor:#E1E1E1;" x='0' y= '0'/>

			<COLORPALETTE>
				<COLOR id='1' order='1' name="7cb5ec" value="#7cb5ec"/>
				<COLOR id='2' order='2' name="434348" value="#434348"/>
				<COLOR id='3' order='3' name="90ed7d" value="#90ed7d"/>
				<COLOR id='4' order='4' name="f7a35c" value="#f7a35c"/>
				<COLOR id='5' order='5' name="8085e9" value="#8085e9"/>
				<COLOR id='6' order='6' name="f15c80" value="#f15c80"/>
				<COLOR id='7' order='7' name="e4d354" value="#e4d354"/>
				<COLOR id='8' order='8' name="2b908f" value="#2b908f"/>
				<COLOR id='9' order='9' name="f45b5b" value="#f45b5b"/>
				<COLOR id='10' order='10' name="91e8e1" value="#91e8e1"/>
			</COLORPALETTE>
			
			<AXES_LIST>
				
				<AXIS type='Serie' style="rotate:;align:;color:#002E57;fontFamily:Arial;fontWeight:normal;fontSize:12px;opposite:false;">
					<MAJORGRID interval="" style="typeline:;color:#D8D8D8;"/>
					<MINORGRID interval="" style="typeline:;color:#E0E0E0;"/>
					<TITLE style="align:;color:#C80752;fontFamily:Arial;fontWeight:bold;fontSize:14px;" text="Title for Y axis"/>
				</AXIS>
   
				<AXIS type='Category' style="rotate:;align:;color:#002E57;fontFamily:Arial;fontWeight:normal;fontSize:12px;">
					<TITLE style="rotate:;align:;color:#C80752;fontFamily:Arial;fontWeight:bold;fontSize:14px;" text="Title for X axis"/>
				</AXIS>
			
			</AXES_LIST>
			
			<VALUES>					
				<SERIE color="" postfixChar="" precision="2" prefixChar="" showValue="true">
					<TOOLTIP backgroundColor=""
						style="color:#C80752;fontFamily:Verdana;fontWeight:;fontSize:10px;align:left;" templateHtml=""/>
				</SERIE>
			</VALUES>

		</CHART>
		
	</generic>
	
	<wordcloud>
		<CHART type="WORDCLOUD" height="" width="" orientation="horizontal" style="fontFamily:Impact;fontSize:12px;fontWeight:normal;backgroundColor:#FAFAFA;" maxAngle='360' maxFontSize='100' maxWords='60' minAngle='0' sizeCriteria='serie' wordPadding='5'/>
	</wordcloud>
	
	<bar>
		<CHART type="BAR"/>
	</bar>

	<line>
		<CHART type="LINE"/>
	</line>

	<pie>
		<CHART type="PIE"/>
	</pie>
	
	<treemap>
		<CHART type="TREEMAP"/>
	</treemap>
	
	<parallel>
		<CHART type="PARALLEL">
			
			<AXES_LIST style="axisColNamePadd:16;brushWidth:10;axisColor:transparent;brushColor:transparent;">
				
				<AXIS type='Serie' style="rotate:;align:;color:#002E57;fontFamily:Arial;fontWeight:normal;fontSize:12px;"/>
			   
				<AXIS type='Category' style="color:#002E57;fontFamily:Arial;fontWeight:normal;fontSize:12px;"/>		
			</AXES_LIST>

			<LIMIT style="maxNumberOfLines:20;orderTopMinBottomMax:;serieFilterColumn:;"/>
			
			<PARALLEL_TOOLTIP style="fontFamily:;fontSize:;minWidth:;maxWidth:;minHeight:;maxHeight:;padding:;border:1;borderRadius:1;"/>
			
			<LEGEND>
				<TITLE style="fontFamily:Arial;fontSize:18px;fontWeight:bold;"/>
				<ELEMENT style="fontFamily:Arial;fontSize:14px;fontWeight:normal;"/>
			</LEGEND>
		
		</CHART>
	
	</parallel>

	<heatmap>
		<CHART type="HEATMAP">
			<LEGEND symbolHeight='400' style="align:middle;"/>
			<TOOLTIP style="fontFamily:Arial;fontSize:12px;color:#002E57;fontWeight:bold;"/>
		</CHART>
	</heatmap>

	<radar>
		<CHART type="RADAR"/>
	</radar>

	<scatter>
		
		<CHART type="SCATTER" zoomType="xy">
		
			<AXES_LIST>
				<AXIS type='Serie' style="rotate:;align:;color:#002E57;fontFamily:Arial;fontWeight:normal;fontSize:12px;opposite:false;">
					<MAJORGRID interval="", style="typeline:;color:#D8D8D8;"/>
					<MINORGRID interval="" typeline="typeline:;color:#E0E0E0;"/>
					<TITLE style="align:;color:#C80752;fontFamily:Arial;fontWeight:bold;fontSize:14px;" text=""/>
				</AXIS>
			
				<AXIS type='Category' style="rotate:;align:;color:#002E57;fontFamily:Arial;fontWeight:normal;fontSize:12px;" startOnTick="false" showLastLabel="true" endOnTick="false">
					<TITLE style="align:;color:#C80752;fontFamily:Arial;fontWeight:bold;fontSize:14px;" text=""/>
				</AXIS>
		
			</AXES_LIST>
		
		</CHART>
	
	</scatter>
	
	<gauge>
		
		<CHART type="GAUGE">
	
			<AXES_LIST>
	
				<AXIS lineColor="#002E57" lineWidth="1" max=""  min="" minorTickColor="#C80752" minorTickLength="5" minorTickPosition="inside" minorTickWidth="1"
		              offset="1" minorTickInterval="0.2" tickColor="#002E57" tickLength="10" type='Serie'
		              tickPixelInterval="10" tickPosition="inside" tickWidth="2" style="rotate:;align:;color:#002E57;fontFamily:Arial;fontWeight:normal;fontSize:14px;opposite:false;">
					
					<TITLE style="align:;color:#002E57;fontFamily:Arial;fontWeight:bold;fontSize:14px;" text=""/>
			
					<LABELS distance="5" rotation="1"/>
			
					<PLOTBANDS>
						<PLOT color="#002E57" from="20" to="40"/>
					</PLOTBANDS>
				
				</AXIS>
		
				<AXIS type='Category' style="rotate:;align:;color:#002E57;fontFamily:Arial;fontWeight:bold;fontSize:14px;">
					<TITLE style="align:;color:#C80752;fontFamily:Arial;fontWeight:bold;fontSize:14px;" text=""/>
				</AXIS>
		
			</AXES_LIST>
	
			<PANE endAngle="121" startAngle="-121"/>
	
			<VALUES>

				<SERIE animation="true" color="#0000FF" pointInterval="1" postfixChar="" precision="" prefixChar="" showInLegend="true"
	                   showValue="true">
					<TOOLTIP backgroundColor="#EEDDAA" style="color:#006699;fontFamily:Verdana;fontWeight:bold;fontSize:14px;align:left" templateHtml="AAA"/>
					<DIAL backgroundColorDial=""/>
					<DATA_LABELS colorDataLabels="#" formatDataLabels="" yPositionDataLabels=""/>
				</SERIE>
			
			</VALUES>
			
		</CHART>
	</gauge>

	<sunburst>
		<CHART type="SUNBURST" opacMouseOver="10">
			<TOOLBAR  style="position:bottom;height:50;width:60;spacing:5;tail:10;percFontColor:#002E57;fontFamily:Arial;fontWeight:normal;fontSize:14px;"/>
			<TIP style="fontFamily:Arial;fontWeight:bold;fontSize:14px;color:#C80752;align:;width:200;" text="Insert text here"/>
		</CHART>
	</sunburst>

	<chord>
		<CHART type="CHORD">
			<TOOLTIP style="fontFamily:Arial;fontSize:16px;border:1;borderRadius:1;"/>
		</CHART>
    </chord>
</TEMPLATE>